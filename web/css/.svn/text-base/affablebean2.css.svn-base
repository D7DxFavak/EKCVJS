/* -------------------------------------------------------------------------------------- */
/* NAKUPNI KOSIK - NAKOUPENE ITEMY */
/* -------------------------------------------------------------------------------------- */

#singleColumn {
    background: none repeat scroll 0 0 rgba(255, 255, 255, 0.3);
    width: 95%;
    margin: 10px auto;
    clear: both;
}

#singleColumn p {
    margin-left: 6px;
}

#actionBar {
    margin: 2px 0px 0px 83px;
    height: 40px;
}

#actionBar a:link, #actionBar a:visited, #actionBar a:hover, #actionBar a:active {
    float: left;
    margin: 6px 4px 6px 6px;
    padding: 2px 4px;
    background: rgba(35, 25, 100, 0.6);
    text-decoration: none;
}

#singleColumn h4 {
    background: none repeat scroll 0 0 rgba(35, 25, 100, 0.6);
    color: white;
    clear: both;
    float: right;
    margin-right: 88px;
    padding: 4px;
}

#cartTable {
    clear: both;
    color: black;
    background: none repeat scroll 0 0 #FEECBF;
    /*background-color: rgba(55, 45, 105, 0.7);*/
    font-size: 1em;
    margin: 8px auto;
}

#cartTable th {
    clear: both;
    background-color: rgba(255, 132, 0, 0.5);
}

#cartTable td {
    color: black;
    background-color: #E2D7B6;
    font-size: 1em;
    padding: 2px 6px;
    font-weight: bold;
}

